    "# 🔥 تابع اصلی OCR فوق‌پیشرفته\n",
    "def ultimate_persian_ocr(pdf_path, processing_method='ultra_high_quality', dpi=400, max_workers=3):\n",
    "    \"\"\"OCR فوق‌پیشرفته فارسی با حداکثر دقت\"\"\"\n",
    "    \n",
    "    print(f\"🔥 شروع Ultimate Persian OCR\")\n",
    "    print(f\"📄 فایل: {os.path.basename(pdf_path)}\")\n",
    "    print(f\"⚙️ تنظیمات: {processing_method}, DPI={dpi}, Workers={max_workers}\")\n",
    "    \n",
    "    start_time = time.time()\n",
    "    \n",
    "    # ایجاد پردازشگرها\n",
    "    image_processor = AdvancedImageProcessor()\n",
    "    text_corrector = PersianTextCorrector()\n",
    "    \n",
    "    # پوشه موقت\n",
    "    temp_dir = '/tmp/ultimate_ocr'\n",
    "    if os.path.exists(temp_dir):\n",
    "        shutil.rmtree(temp_dir)\n",
    "    os.makedirs(temp_dir)\n",
    "    \n",
    "    try:\n",
    "        # تبدیل PDF به تصاویر با کیفیت بالا\n",
    "        print(\"\\n🖼️ تبدیل PDF به تصاویر با کیفیت فوق‌العاده...\")\n",
    "        images = convert_from_path(\n",
    "            pdf_path, \n",
    "            dpi=dpi, \n",
    "            fmt='png',  # PNG برای کیفیت بهتر\n",
    "            thread_count=2,\n",
    "            use_pdftocairo=True  # کیفیت رندر بهتر\n",
    "        )\n",
    "        \n",
    "        print(f\"📊 {len(images)} صفحه با کیفیت {dpi} DPI آماده شد\")\n",
    "        \n",
    "        # ذخیره تصاویر با پیش‌پردازش اولیه\n",
    "        print(\"🎨 پیش‌پردازش و ذخیره تصاویر...\")\n",
    "        image_data = []\n",
    "        \n",
    "        for i, img in enumerate(tqdm(images, desc=\"آماده‌سازی\")):\n",
    "            # تعیین scale factor بر اساس سایز تصویر\n",
    "            width, height = img.size\n",
    "            if width < 1500:  # تصاویر کوچک\n",
    "                scale_factor = 3.5\n",
    "            elif width < 2500:  # تصاویر متوسط\n",
    "                scale_factor = 2.5\n",
    "            else:  # تصاویر بزرگ\n",
    "                scale_factor = 2.0\n",
    "            \n",
    "            # پیش‌پردازش تصویر\n",
    "            processed_img = image_processor.process(img, processing_method, scale_factor)\n",
    "            \n",
    "            # ذخیره\n",
    "            img_path = os.path.join(temp_dir, f\"page_{i+1:03d}.png\")\n",
    "            processed_img.save(img_path, 'PNG', optimize=True)\n",
    "            \n",
    "            image_data.append({\n",
    "                'path': img_path,\n",
    "                'page': i + 1,\n",
    "                'original_size': img.size,\n",
    "                'processed_size': processed_img.size,\n",
    "                'scale_factor': scale_factor\n",
    "            })\n",
    "            \n",
    "            # پاکسازی حافظه\n",
    "            del img, processed_img\n",
    "        \n",
    "        del images\n",
    "        gc.collect()\n",
    "        \n",
    "        # تابع OCR هر صفحه\n",
    "        def process_single_page(img_info):\n",
    "            try:\n",
    "                page_start = time.time()\n",
    "                \n",
    "                # بارگذاری تصویر\n",
    "                img = Image.open(img_info['path'])\n",
    "                \n",
    "                # تنظیمات OCR حرفه‌ای\n",
    "                ocr_configs = [\n",
    "                    '--psm 3 --oem 3',  # صفحه کامل\n",
    "                    '--psm 4 --oem 3',  # ستون واحد\n",
    "                    '--psm 6 --oem 3'   # بلوک واحد\n",
    "                ]\n",
    "                \n",
    "                best_text = \"\"\n",
    "                best_confidence = 0\n",
    "                \n",
