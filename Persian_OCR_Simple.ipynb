    "# 🧠 کلاس تصحیح متن فارسی\n",
    "class PersianTextCorrector:\n",
    "    def __init__(self):\n",
    "        # دیکشنری تصحیح کاراکترهای رایج\n",
    "        self.char_corrections = {\n",
    "            # اعداد فارسی به انگلیسی\n",
    "            '۰': '0', '۱': '1', '۲': '2', '۳': '3', '۴': '4',\n",
    "            '۵': '5', '۶': '6', '۷': '7', '۸': '8', '۹': '9',\n",
    "            \n",
    "            # تصحیح حروف مشابه\n",
    "            'ي': 'ی', 'ك': 'ک', 'ء': 'ٔ',\n",
    "            'أ': 'آ', 'إ': 'ا', 'ة': 'ه',\n",
    "            \n",
    "            # حذف کاراکترهای اضافی\n",
    "            '\\u200c\\u200c': '\\u200c',  # نیم‌فاصله مضاعف\n",
    "            '  ': ' ',  # فاصله مضاعف\n",
    "        }\n",
    "        \n",
    "        # الگوهای رایج املایی\n",
    "        self.word_corrections = {\n",
    "            'مي\\u200cشود': 'می\\u200cشود',\n",
    "            'مي\\u200cتوان': 'می\\u200cتوان',\n",
    "            'مي\\u200cکند': 'می\\u200cکند',\n",
    "            'نمي\\u200c': 'نمی\\u200c',\n",
    "            'بي\\u200c': 'بی\\u200c',\n",
    "        }\n",
    "    \n",
    "    def correct_text(self, text):\n",
    "        \"\"\"تصحیح جامع متن فارسی\"\"\"\n",
    "        if not text:\n",
    "            return text\n",
    "        \n",
    "        # نرمال‌سازی Unicode\n",
    "        text = unicodedata.normalize('NFKC', text)\n",
    "        \n",
    "        # تصحیح کاراکترهای تک\n",
    "        for wrong, correct in self.char_corrections.items():\n",
    "            text = text.replace(wrong, correct)\n",
    "        \n",
    "        # تصحیح کلمات رایج\n",
    "        for wrong_pattern, correct in self.word_corrections.items():\n",
    "            text = re.sub(wrong_pattern, correct, text)\n",
    "        \n",
    "        # تصحیح فاصله‌گذاری\n",
    "        text = self._fix_spacing(text)\n",
    "        \n",
    "        # تصحیح نقطه‌گذاری\n",
    "        text = self._fix_punctuation(text)\n",
    "        \n",
    "        # تمیزکاری نهایی\n",
    "        text = self._final_cleanup(text)\n",
    "        \n",
    "        return text\n",
    "    \n",
    "    def _fix_spacing(self, text):\n",
    "        \"\"\"تصحیح فاصله‌گذاری\"\"\"\n",
    "        # حذف فاصله‌های اضافی\n",
    "        text = re.sub(r'\\s+', ' ', text)\n",
    "        \n",
    "        # اصلاح نیم‌فاصله\n",
    "        text = re.sub(r'\\s+\\u200c\\s+', '\\u200c', text)\n",
    "        text = re.sub(r'\\u200c\\s+', '\\u200c', text)\n",
    "        text = re.sub(r'\\s+\\u200c', '\\u200c', text)\n",
    "        \n",
    "        return text\n",
    "    \n",
    "    def _fix_punctuation(self, text):\n",
    "        \"\"\"تصحیح نقطه‌گذاری\"\"\"\n",
    "        # فاصله قبل از نقطه‌گذاری\n",
    "        text = re.sub(r'\\s+([.،؛:!؟])', r'\\1', text)\n",
    "        \n",
    "        # فاصله بعد از نقطه‌گذاری\n",
    "        text = re.sub(r'([.،؛:!؟])([^\\s])', r'\\1 \\2', text)\n",
    "        \n",
    "        return text\n",
    "    \n",
    "    def _final_cleanup(self, text):\n",
    "        \"\"\"تمیزکاری نهایی\"\"\"\n",
    "        # حذف خطوط خالی اضافی\n",
    "        text = re.sub(r'\\n\\s*\\n', '\\n\\n', text)\n",
    "        \n",
    "        # تریم کردن\n",
    "        text = text.strip()\n",
    "        \n",
    "        return text\n",
    "    \n",
    "    def analyze_quality(self, text):\n",
    "        \"\"\"تحلیل کیفیت متن\"\"\"\n",
    "        if not text:\n",
    "            return {'score': 0, 'issues': ['متن خالی']}\n",
    "        \n",
    "        issues = []\n",
    "        score = 100\n",
    "        \n",
    "        # بررسی کاراکترهای غیرمعمول\n",
    "        unusual_chars = re.findall(r'[^\\u0600-\\u06FF\\u200C\\u200D\\s\\d\\w.,;:!?()\\[\\]{}\"\\'-]', text)\n",
    "        if unusual_chars:\n",
    "            score -= len(set(unusual_chars)) * 2\n",
    "            issues.append(f'کاراکترهای غیرمعمول: {\", \".join(set(unusual_chars[:5]))}')\n",
    "        \n",
    "        # بررسی نسبت حروف فارسی\n",
    "        persian_chars = len(re.findall(r'[\\u0600-\\u06FF]', text))\n",
    "        total_chars = len(re.findall(r'\\S', text))\n",
    "        \n",
    "        if total_chars > 0:\n",
    "            persian_ratio = persian_chars / total_chars\n",
    "            if persian_ratio < 0.7:\n",
    "                score -= 20\n",
    "                issues.append(f'نسبت حروف فارسی پایین: {persian_ratio:.1%}')\n",
    "        \n",
    "        # بررسی فاصله‌گذاری\n",
    "        spacing_issues = len(re.findall(r'\\s{3,}', text))\n",
    "        if spacing_issues > 0:\n",
    "            score -= spacing_issues\n",
    "            issues.append(f'مشکل فاصله‌گذاری: {spacing_issues} مورد')\n",
    "        \n",
    "        return {\n",
    "            'score': max(0, score),\n",
    "            'issues': issues,\n",
    "            'persian_ratio': persian_ratio if total_chars > 0 else 0,\n",
    "            'total_chars': total_chars\n",
    "        }\n",
    "\n",
    "print(\"🧠 تصحیح‌گر متن فارسی آماده شد!\")"
   ]
